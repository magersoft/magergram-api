{
  "name": "magergram",
  "version": "0.6.6",
  "description": "Instagram clone with NodeJS, Prisma, GraphQL, React and React Native",
  "repository": "https://github.com/magersoft/magergram.git",
  "author": "magersoft <magervlad@yandex.ru>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts -e ts,graphql",
    "generate": "prisma generate",
    "prisma": "yarn run prisma:deploy && yarn run generate",
    "prisma:deploy": "prisma deploy --force",
    "predeploy": "yarn run generate",
    "deploy": "git push heroku master",
    "prebuild": "rm -rf build",
    "build": "tsc",
    "postbuild": "cd src && copy ./api/*.graphql ../build/ && copy ./api/**/*.graphql ../build/ && copy ./api/**/**/*.graphql ../build/",
    "start": "node build/server.js"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.9.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "firebase": "^7.13.1",
    "firebase-admin": "^8.10.0",
    "graphql-tools": "^4.0.7",
    "graphql-upload": "^10.0.0",
    "graphql-yoga": "^1.18.3",
    "is_js": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.6",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.3",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma-client-lib": "^1.34.10",
    "sharp": "^0.25.2",
    "ts-node": "^8.9.0",
    "tslint-config-prettier": "^1.18.0",
    "twilio": "^3.40.0",
    "typescript": "^3.8.3",
    "uuid-v4": "^0.1.0",
    "web-push": "^3.4.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^13.13.2",
    "@types/nodemailer": "^6.4.0",
    "@types/twilio": "^2.11.0",
    "copy": "^0.3.2",
    "gql-merge": "^0.0.6",
    "nodemon": "^2.0.2"
  }
}
